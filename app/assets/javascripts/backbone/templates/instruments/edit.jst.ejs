<h1>Edit instrument</h1>

<form id="edit-instrument" name="instrument">
  <div class="field">
    <label for="prefix"> prefix:</label>
    <input type="text" name="prefix" id="prefix" value="<%= prefix %>" >
  </div>

  <div class="field">
    <label for="port"> port:</label>
    <input type="text" name="port" id="port" value="<%= port %>" >
  </div>

  <div class="field">
    <label for="study"> study:</label>
    <input type="text" name="study" id="study" value="<%= study %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Update Instrument" />
  </div>

</form>

