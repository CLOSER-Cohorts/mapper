<%= link_to 'Edit', '#', class: 'edit', :onclick => 'jQuery("<div />").attr("title","' + variable.name + '").html([jQuery("<label/>").html("Type").attr("for","edit-var-type-' + variable.id.to_s + '"),jQuery("<select/>").attr("id","edit-var-type-' + variable.id.to_s + '").html("<option value=\'Normal\'>Normal</option><option value=\'Derived\'>Derived</option>"),"<br/>",jQuery("<label/>").html("Label").attr("for","edit-var-label-' + variable.id.to_s + '"),jQuery("<input/>").attr("id","edit-var-label-' + variable.id.to_s + '").attr("size","50").val("' + variable.label + '")]).dialog({width: 450})' %>
<%= link_to 'Destroy', variable, method: :delete, tabindex: '-1', class: 'destroy', data: { confirm: 'Are you sure?' } %>
